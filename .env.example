# =========================================================
# LPR GateBox — пример настроек окружения
# Скопируй в .env и заполни значения
# =========================================================

# какой тег образов использовать
# stable = всегда последняя стабильная сборка
TAG=stable

# порты
GATEBOX_PORT=8080
UPDATER_PORT=9010

# RTSP камера
RTSP_URL=rtsp://192.168.2.102:8554/lpr

# MQTT
MQTT_ENABLED=1
MQTT_HOST=m4.wqtt.ru
MQTT_PORT=8854
MQTT_USER=REPLACE_ME
MQTT_PASS=REPLACE_ME
MQTT_TOPIC=gate/open

# Worker FPS
READ_FPS=10
DET_FPS=8
SEND_FPS=6

# YOLO
DET_CONF=0.40
DET_IOU=0.45
DET_IMG_SIZE=416

# ROI (если нет — оставь пустым)
ROI=

# Crop/rectify/refine
PLATE_PAD=0.14
RECTIFY=1
RECTIFY_W=320
RECTIFY_H=96

REFINE_INNER_PAD=0.03
REFINE_MIN_AREA_RATIO=0.03

# debug
DEBUG_SAVE=0
SETTINGS_POLL_SEC=1.5